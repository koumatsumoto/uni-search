{
  "name": "uni-search",
  "version": "0.0.0",
  "scripts": {
    "build": "yarn :build:ng:app --prod && yarn :build:tsc:extensions && yarn :build:copy-css && yarn :build:copy-manifest",
    "format": "prettier --write . && ng lint --fix",
    "start": "ng serve",
    "// 1": "internal scripts",
    ":build:ng:app": "ng build",
    ":build:tsc:extensions": "tsc src/chrome-extensions/*.ts --outDir dist",
    ":build:copy-css": "cp src/chrome-extensions/*.css dist/",
    ":build:copy-manifest": "cp manifest.json dist/manifest.json",
    ":test:e2e": "ng e2e",
    ":test:unit": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "destyle.css": "^1.0.15",
    "neo4j-driver": "^4.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@types/chrome": "^0.0.123",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
